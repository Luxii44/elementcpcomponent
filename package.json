{
  "name": "@luxii/wmcomponent",
  "packageManager": "pnpm@7.26.3",
  "private": false,
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run -C internal/build start",
    "preview": "vite preview",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "typecheck:web": "vue-tsc -p tsconfig.web.json --composite false --noEmit",
    "typecheck:node": "tsc -p tsconfig.node.json --noEmit",
    "typecheck:play": "vue-tsc -p tsconfig.play.json --composite false --noEmit",
    "typecheck:vite-config": "vue-tsc -p tsconfig.vite-config.json --composite false --noEmit"
  },
  "dependencies": {
    "@wmcomponent/components": "workspace:*",
    "@wmcomponent/constants": "workspace:*",
    "@wmcomponent/utils": "workspace:*"
  },
  "peerDependencies": {
    "element-plus": "^2.2.29",
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@esbuild-kit/cjs-loader": "^2.4.2",
    "@pnpm/find-workspace-packages": "^5.0.33",
    "@pnpm/logger": "^5.0.0",
    "@pnpm/types": "^8.10.0",
    "@types/fs-extra": "^11.0.1",
    "@types/gulp": "^4.0.10",
    "@types/jsdom": "^21.1.0",
    "@types/node": "*",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/runtime-core": "^3.2.47",
    "@vue/tsconfig": "^0.1.3",
    "@wmcomponent/build": "workspace:^0.0.1",
    "@wmcomponent/build-utils": "workspace:^0.0.1",
    "chalk": "^5.2.0",
    "consola": "^2.15.3",
    "element-plus": "^2.2.29",
    "gulp": "^4.0.2",
    "rimraf": "^4.1.2",
    "rollup": "^3.15.0",
    "ts-morph": "^14.0.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.14.2",
    "unplugin-vue-components": "^0.23.0",
    "unplugin-vue-define-options": "^1.2.1",
    "unplugin-vue-macros": "^1.7.3",
    "vite": "^4.1.0",
    "vue": "^3.2.47",
    "vue-tsc": "^1.0.24"
  },
  "workspaces": [
    "packages/*"
  ],
  "types": ".dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "import": "./dist/wmcomponent.js",
      "require": "./dist/wmcomponent.umd.cjs"
    },
    "./package.json": "./dist/package.json"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite"
      ]
    }
  }
}